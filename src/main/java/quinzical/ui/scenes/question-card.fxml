<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" prefWidth="200.0" style="-fx-background-color: #38383d; -fx-background-radius: 10;">
         <children>
            <Label fx:id="category_name" style="-fx-background-color: #38383daa; -fx-text-fill: white; -fx-padding: 5;" text="QUESTION">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Label>
            <VBox fx:id="questions" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </VBox>
   </children>
</Pane>
