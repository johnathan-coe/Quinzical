<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="810.0" prefWidth="600.0" styleClass="background" stylesheets="@../css/ui.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" styleClass="body">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label alignment="CENTER" style="-fx-background-color: #38383daa; -fx-text-fill: white; -fx-padding: 10;" text="Quinzical" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font size="80.0" />
                     </font>
                  </Label>
                  <Label fx:id="currentScoreLabel" text="You currently have $???" textFill="#ffe900" VBox.vgrow="NEVER">
                     <font>
                        <Font size="20.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" left="15.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
            <VBox maxHeight="400.0" prefHeight="200.0" prefWidth="600.0" spacing="10.0" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="playButton" mnemonicParsing="false" onAction="#playPressed" styleClass="normal-button" text="Play">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="practiceButton" mnemonicParsing="false" onAction="#practicePressed" styleClass="normal-button" text="Practice">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="practiceButton1" mnemonicParsing="false" onAction="#leaderboardPressed" styleClass="normal-button" text="Leaderboard">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="resetButton" mnemonicParsing="false" onAction="#resetPressed" styleClass="normal-button" text="Reset">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#settingsPressed" styleClass="normal-button" text="Settings">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#helpPressed" styleClass="normal-button" text="Help">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
